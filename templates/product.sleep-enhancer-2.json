    "hero_benefits_L86ixD": {
      "type": "hero-benefits",
      "blocks": {
        "benefit_item_Rw9Kdc": {
          "type": "benefit_item",
          "settings": {
            "label": "Calmer Evenings",
            "icon_type": "image",
            "icon": "{{ product.featured_image }}",
            "icon_svg": "",
            "icon_google_name": "check_circle",
            "description": "The Sleep Enhancer Lens filters out the sharp blue‑green spike from phones, TVs, and overhead LEDs so your visual system is not stuck in “daytime” mode. With that harsh light turned down, your eyes and nervous system can actually start to relax instead of feeling wired and overworked late into the night.",
            "reviewer_name": "Sarah M",
            "star_rating": 5,
            "review_text": "“I’m a full‑time designer and by 4pm my eyes used to feel cooked from Figma and my phone. With Parlay on all day, that burning, gritty feeling is basically gone.”",
            "hide_review_box": false,
            "block_image_position": "above_review",
            "block_image_aspect_ratio": "16/9",
            "block_image_border_radius": 10,
            "block_image_border_width": 0,
